<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="351.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Main">
   <children>
      <VBox alignment="CENTER" prefHeight="351.0" prefWidth="411.0" spacing="12.0">
         <children>
            <Label alignment="TOP_CENTER" contentDisplay="TOP" text="Welcome! Please Login">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="28.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="-5.0" />
               </VBox.margin>
            </Label>
            <GridPane alignment="CENTER" prefHeight="90.0" prefWidth="273.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="10.0" prefWidth="70.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="10.0" prefWidth="130.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="39.0" minHeight="10.0" prefHeight="39.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="46.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label alignment="CENTER" text="User Name">
                     <font>
                        <Font name="JetBrains Mono Medium" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Password" GridPane.rowIndex="1">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="userName" prefHeight="33.0" prefWidth="150.0" promptText="Enter User Name" GridPane.columnIndex="1" />
                  <PasswordField fx:id="password" prefHeight="34.0" prefWidth="178.0" promptText="Enter Password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <HBox alignment="CENTER" prefHeight="98.0" prefWidth="181.0" spacing="30.0">
               <children>
                  <Button fx:id="login" mnemonicParsing="false" prefHeight="40.0" prefWidth="75.0" text="Login">
                     <font>
                        <Font name="JetBrains Mono SemiBold" size="12.0" />
                     </font>
                  </Button>
                  <Button fx:id="exit" mnemonicParsing="false" onAction="#onActionExit" prefHeight="40.0" prefWidth="75.0" text="Exit">
                     <font>
                        <Font name="JetBrains Mono Medium" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="location" text="Location">
               <font>
                  <Font name="JetBrains Mono SemiBold" size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
